{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",

    "rules": {
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-console": 1
    }
}